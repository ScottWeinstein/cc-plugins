{
  "name": "wt-dev",
  "version": "0.1.0",
  "description": "Unified worktree and Inngest dev server management for multi-worktree projects",
  "type": "module",
  "bin": {
    "wt-dev": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "skills"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "worktree",
    "git",
    "dev-server",
    "inngest",
    "cli",
    "claude-code"
  ],
  "author": "Scott Weinstein",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
