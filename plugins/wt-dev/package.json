{
  "name": "wt-dev",
  "version": "0.1.2",
  "description": "Unified worktree and Inngest dev server management for multi-worktree projects",
  "type": "module",
  "bin": {
    "wt-dev": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "skills",
    ".claude-plugin"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build",
    "postinstall": "node dist/register.js || true"
  },
  "keywords": [
    "worktree",
    "git",
    "dev-server",
    "inngest",
    "cli",
    "claude-code"
  ],
  "author": "Scott Weinstein",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScottWeinstein/cc-plugins.git",
    "directory": "plugins/wt-dev"
  },
  "bugs": {
    "url": "https://github.com/ScottWeinstein/cc-plugins/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "vitest": "^4.0.15"
  }
}
